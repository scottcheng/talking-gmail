{
  "name": "__MSG_extName__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_extDesc__",
  "permissions": [
    "tts"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://mail.google.com/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "lib/jquery.min.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "default_locale": "en",
  "icons": {
  }
}
